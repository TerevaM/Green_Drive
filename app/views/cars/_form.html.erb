  <%= form_with model: @car do |f| %>
    <%= f.label :model %>
    <%= f.text_field :model %>
    <%= f.label :availability %>
    <%= f.text_field :availability %>
    <%= f.label :brand %>
    <%= f.text_field :brand %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%# changer l'image url en download image %>
    <%= f.label :image_url %>
    <%= f.text_field :image_url %>
    <%= f.submit %>
  <% end %>
