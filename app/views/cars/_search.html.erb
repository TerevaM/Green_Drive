<%= search_form_for @q, url: cars_path, class: 'form_search', method: :get do |f| %>
  <%= f.text_field :location_cont, placeholder: "Localisation" %>
  <%= f.date_field("start_date", min: Date.today + 1, value: Date.today + 1)%>
  <%= f.date_field("end_date", min: Date.today + 2, value: Date.today + 2)%>
  <%= f.submit "Rechercher" %>
<% end %>
